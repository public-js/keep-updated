{
  "name": "keep-updated",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "wf:prettier": "prettier '**/*.{js,ts,scss,html,md,json,yml}'",
    "cd:root:lint": "eslint '*.{js,ts}' --no-ignore",
    "cd:ver-bump": "standard-version",
    "cd:gh-release": "cc-github-releaser --prefix v",
    "cd:afd:test": "nx affected --target test --parallel",
    "cd:afd:build": "nx affected --target build --parallel",
    "cd:all:lint": "nx run-many --target lint --all --parallel",
    "cd:all:test": "nx run-many --target test --all --parallel --codeCoverage --coverageReporters=lcov",
    "cd:all:build": "nx run-many --target build --all --parallel",
    "cd:all:publish": "nx run-many --target publish --all",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.0",
    "@commitlint/config-conventional": "^16.2.0",
    "@nrwl/cli": "13.8.1",
    "@nrwl/eslint-plugin-nx": "13.8.1",
    "@nrwl/jest": "13.8.1",
    "@nrwl/js": "13.8.1",
    "@nrwl/linter": "13.8.1",
    "@nrwl/node": "13.8.1",
    "@nrwl/nx-cloud": "13.1.4",
    "@nrwl/tao": "13.8.1",
    "@nrwl/workspace": "13.8.1",
    "@public-js/cc-github-releaser": "^1.1.0",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "~5.10.0",
    "@typescript-eslint/parser": "~5.10.0",
    "eslint": "8.7.0",
    "eslint-config-custom": "file:tools/eslint-config-custom",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.10.0",
    "eslint-plugin-import": "^2.25.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.0",
    "jest": "27.2.3",
    "lint-staged": "^12.3.0",
    "prettier": "^2.5.0",
    "standard-version": "^9.3.2",
    "ts-jest": "27.0.5",
    "typescript": "~4.5.2"
  },
  "license": "MIT",
  "author": "Public JS <public-js@muffin.blue> (https://github.com/public-js/public-js)",
  "repository": "https://github.com/public-js/keep-updated",
  "homepage": "https://github.com/public-js/keep-updated",
  "lint-staged": {
    "*.{js,ts,css,scss,html,md,json}": "prettier --write",
    "*.{js,ts}": "eslint --fix"
  },
  "engines": {
    "node": ">=10.3.0"
  }
}
